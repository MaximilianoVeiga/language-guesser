{
  "name": "language-guesser",
  "version": "0.0.1",
  "description": "TypeScript compatible language guesser based on n-grams",
  "type": "module",
  "license": "MIT",
  "types": "src/index.d.ts",
  "main": "dist/cjs/index.js",
  "module": "src/index.js",
  "scripts": {
    "build-cjs": "babel src --out-dir dist/cjs --presets=@babel/preset-env",
    "build-esm": "babel src --out-dir dist/esm --presets=@babel/preset-modules"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.5",
    "@babel/plugin-proposal-unicode-property-regex": "^7.18.6",
    "@babel/plugin-syntax-import-attributes": "^7.24.1",
    "@babel/preset-env": "^7.24.5"
  }
}
